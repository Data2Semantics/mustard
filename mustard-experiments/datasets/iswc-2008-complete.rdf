        <swrc:url rdf:resource="http://sunsite.informatik.rwth-aachen.de/Publications/CEUR-WS/Vol-401/iswc2008pd_submission_17.pdf"/>
        <swrc:year>2008</swrc:year>
        <swrc_ext:authorList rdf:parseType="Resource">
