            <rdf:_2 rdf:resource="http://data.semanticweb.org/person/frank-van-harmelen"/>
        </swrc_ext:authorList>
        <rdfs:label>Using Semantic Distances for Reasoning with Inconsistent Ontologies</rdfs:label>
