    <swrc:InProceedings rdf:about="http://data.semanticweb.org/conference/iswc/2008/paper/poster_demo/23">
        <swc:isPartOf rdf:resource="http://data.semanticweb.org/conference/iswc/2008/poster_demo_proceedings"/>
