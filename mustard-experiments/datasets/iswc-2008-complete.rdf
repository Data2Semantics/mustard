    <swrc:InProceedings rdf:about="http://data.semanticweb.org/conference/iswc/2008/paper/in_use/17">
        <swc:isPartOf rdf:resource="http://data.semanticweb.org/conference/iswc/2008/proceedings"/>
        <dc:creator rdf:resource="http://data.semanticweb.org/person/david-martin"/>
