        <swrc:author rdf:resource="http://data.semanticweb.org/person/michael-wolverton"/>
        <swrc:month>October</swrc:month>
        <swrc:year>2008</swrc:year>
        <swrc_ext:authorList rdf:parseType="Resource">
            <rdf:_1 rdf:resource="http://data.semanticweb.org/person/michael-wolverton"/>
