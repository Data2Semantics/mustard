    <swrc:InProceedings rdf:about="http://data.semanticweb.org/conference/iswc/2009/paper/research/347">
        <swc:hasTopic rdf:resource="http://dbpedia.org/resource/Semantic_Web"/>
        <swc:isPartOf rdf:resource="http://data.semanticweb.org/conference/iswc/2009/proceedings"/>
