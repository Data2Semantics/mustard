    <swrc:InProceedings rdf:about="http://data.semanticweb.org/conference/eswc/2009/paper/101">
        <hasRelatedEvent rdf:resource="http://data.semanticweb.org/conference/eswc/2009/track/research/talk/101"/>
        <hasTopic rdf:resource="http://dbpedia.org/resource/Ontology_%28computer_science%29"/>
