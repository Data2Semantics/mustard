        <rdfs:label>Debugging the missing is-a structure of networked ontologies</rdfs:label>
        <maker rdf:resource="http://data.semanticweb.org/person/patrick-lambrix"/>
        <maker rdf:resource="http://data.semanticweb.org/person/qiang-liu"/>
    </swrc:InProceedings>

