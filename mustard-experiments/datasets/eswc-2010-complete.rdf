    <swrc:InProceedings rdf:about="http://data.semanticweb.org/conference/eswc/2010/paper/web_of_data/28">
        <swc:isPartOf rdf:resource="http://data.semanticweb.org/conference/eswc/2010/proceedings"/>
        <dc:creator rdf:resource="http://data.semanticweb.org/person/isabel-rojas"/>
