        <srwc-ext:authorList rdf:resource="http://data.semanticweb.org/conference/iswc/2011/paper/poster-demo/55/authorlist"/>
        <rdfs:label>Semantic Smoothing for Twitter Sentiment Analysis</rdfs:label>
