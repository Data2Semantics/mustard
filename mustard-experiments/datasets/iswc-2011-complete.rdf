    <swrc:InProceedings rdf:about="http://data.semanticweb.org/conference/iswc/2011/paper/doctoral-consortium/14">
        <swc:hasTopic rdf:resource="http://dbpedia.org/resource/Semantic_Web"/>
        <swc:relatedToEvent rdf:resource="http://data.semanticweb.org/conference/iswc/2011/talks/doctoral-consortium/14"/>
        <dc:creator rdf:resource="http://data.semanticweb.org/person/varish-mulwad"/>
        <dc:subject>Entity Linking</dc:subject>
