    <swrc:author rdf:resource="http://data.semanticweb.org/person/fabien-duchateau"/>
    <swrc:author rdf:resource="http://data.semanticweb.org/person/trond-aalberg"/>
    <swc:isPartOf rdf:resource="http://data.semanticweb.org/conference/iswc/2012/proceedings"/>
    <bibo:authorList rdf:resource="http://data.semanticweb.org/conference/iswc/2012/paper/research-37/authors"/>
    <swrc:idLncs>76490561</swrc:idLncs>
