  <rdf:Description rdf:about="http://data.semanticweb.org/conference/iswc/2012/paper/poster-demo-27">
    <dcterms:subject>Domain Entity Annotation</dcterms:subject>
    <swc:relatedToEvent rdf:resource="http://data.semanticweb.org/conference/iswc/2012/special/poster-demo-swc-reception"/>
