    <dcterms:creator rdf:resource="http://data.semanticweb.org/person/raphael-troncy"/>
    <swrc:author rdf:resource="http://data.semanticweb.org/person/raphael-troncy"/>
    <dc:creator rdf:resource="http://data.semanticweb.org/person/raphael-troncy"/>
