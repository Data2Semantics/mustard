    <dcterms:creator rdf:resource="http://data.semanticweb.org/person/ulrike-sattler"/>
    <swrc:listKeyword>Justifications,Justification Finding,Explanation,Blackbox Debugging</swrc:listKeyword>
    <swrc:author rdf:resource="http://data.semanticweb.org/person/ulrike-sattler"/>
    <dc:title>Extracting Justifications from BioPortal Ontologies</dc:title>
