  <rdf:Description rdf:about="http://data.semanticweb.org/conference/iswc/2012/paper/inuse-18">
    <swrc:author rdf:resource="http://data.semanticweb.org/person/jane-hunter"/>
    <dc:creator rdf:resource="http://data.semanticweb.org/person/andreas-zankl"/>
