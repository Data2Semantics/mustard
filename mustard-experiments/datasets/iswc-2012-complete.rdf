    <swrc:listAuthor>Thanos G. Stavropoulos, Dimitris Vrakas and Ioannis Vlahavas</swrc:listAuthor>
    <swrc:author rdf:resource="http://data.semanticweb.org/person/ioannis-vlahavas"/>
