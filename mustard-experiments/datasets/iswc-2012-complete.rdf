    <foaf:maker rdf:resource="http://data.semanticweb.org/person/gerhard-friedrich"/>
    <rdf:type rdf:resource="http://swrc.ontoware.org/ontology#InProceedings"/>
    <swrc:listAuthor>Kostyantyn Shchekotykhin, Patrick Rodler, Philipp Fleiss and Gerhard Friedrich</swrc:listAuthor>
    <dcterms:subject>diagnosis discrimination</dcterms:subject>
    <swc:relatedToEvent rdf:resource="http://data.semanticweb.org/conference/iswc/2012/special/poster-demo-swc-reception"/>
