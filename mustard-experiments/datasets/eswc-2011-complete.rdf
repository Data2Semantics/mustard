    <swrc:InProceedings rdf:about="http://data.semanticweb.org/conference/eswc/2011/paper/social-web/8">
        <swc:isPartOf rdf:resource="http://data.semanticweb.org/conference/eswc/2011/proceedings"/>
        <dc:creator rdf:resource="http://data.semanticweb.org/person/andreas-hotho"/>
        <dc:creator rdf:resource="http://data.semanticweb.org/person/christian-koerner"/>
        <dc:creator rdf:resource="http://data.semanticweb.org/person/dominik-benz"/>
