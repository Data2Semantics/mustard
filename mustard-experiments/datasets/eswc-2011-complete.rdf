    </swrc:InProceedings>

    <rdf:Description rdf:about="http://data.semanticweb.org/conference/eswc/2011/paper/semantic-data-management/23/authorlist">
        <rdf:_1 rdf:resource="http://data.semanticweb.org/person/shady-elbassuoni"/>
