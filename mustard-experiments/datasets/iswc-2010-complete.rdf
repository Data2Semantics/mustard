    <swrc:InProceedings rdf:about="http://data.semanticweb.org/conference/iswc/2010/paper/131">
        <bibo:authorList rdf:resource="http://data.semanticweb.org/conference/iswc/2010/paper/131/authorlist"/>
        <swc:isPartOf rdf:resource="http://data.semanticweb.org/conference/iswc/2010/proceedings"/>
