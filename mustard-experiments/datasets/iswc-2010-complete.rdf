        <maker rdf:resource="http://data.semanticweb.org/person/geert-jan-houben"/>
        <maker rdf:resource="http://data.semanticweb.org/person/jan-hidders"/>
        <maker rdf:resource="http://data.semanticweb.org/person/samur-araujo"/>
    </swrc:InProceedings>

