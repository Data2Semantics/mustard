        <swrc:url rdf:resource="http://iswc2010.semanticweb.org/pdf/510.pdf"/>
        <swrc:year>2010</swrc:year>
        <rdfs:label>xhRank: Ranking Entities on the Semantic Web</rdfs:label>
        <maker rdf:resource="http://data.semanticweb.org/person/mark-baker"/>
