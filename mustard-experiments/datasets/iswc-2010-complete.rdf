    <swrc:InProceedings rdf:about="http://data.semanticweb.org/conference/iswc/2010/paper/463">
        <bibo:authorList rdf:resource="http://data.semanticweb.org/conference/iswc/2010/paper/463/authorlist"/>
        <swc:isPartOf rdf:resource="http://data.semanticweb.org/conference/iswc/2010/poster-demo-proceedings"/>
