        <maker rdf:resource="http://data.semanticweb.org/person/david-karger"/>
        <maker rdf:resource="http://data.semanticweb.org/person/edward-benson"/>
        <maker rdf:resource="http://data.semanticweb.org/person/fabian-howahl"/>
    </swrc:InProceedings>

